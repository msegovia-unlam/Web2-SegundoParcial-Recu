@model List<BD.ModeloEF.ObraDeArte>
@{
    ViewData["Title"] = "Lista de Obras de Arte del Siglo xix";
}

<div class="text-center">
    <h1 class="display-4">Obras de Arte</h1>
   
    <table>
        <tr>
            <th>Nombre obra de arte</th>
            <th>Año Creacion</th>
            
        </tr>
    @foreach (var obra in Model)
    {
        <tr>
            <td>@obra.Nombre</td>
            <td>@obra.AnioCreacion</td>
            
        </tr>
    }
    </table>
</div>